local rainspawned = 0


while true do
	if rainspawned >= 1000 then
		break
	end
	local RandomNumber = math.random(1,50)
	local RandomNumber2 = math.random(1,50)
	local RandomNumber3 = math.random(1,50)	
	rainspawned = rainspawned + 1
	wait()
	local Rain = Instance.new("Part",game.Workspace)
	Rain.Position = Vector3.new(RandomNumber,15,RandomNumber2)
	Rain.Size = Vector3.new(0.5,2,0.5)
	Rain.Anchored = false
	Rain.Transparency = 0.7	
end

print("Done!")

